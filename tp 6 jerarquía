class Persona:
    def _init_(self, nombre, edad):
        self.nombre = nombre
        self.edad = edad
    def mostrar_info_personal(self):
        print("Nombre: {self.nombre}, Edad: {self.edad}")
class Estudiante(Persona):
    def asignar_rol(self):
        print("{self.nombre} es un estudiante.")

    def mostrar_info(self):
        self.mostrar_info_personal()
        self.asignar_rol()
class Profesor(Persona):
    def asignar_rol(self):
        print("{self.nombre} es un profesor.")

    def mostrar_info(self):
        self.mostrar_info_personal()
        self.asignar_rol()
