class Vehiculo:
    def __init__(self, marca, modelo, año, consumo):
        self.marca = marca
        self.modelo = modelo
        self.año = año
        self.consumo = consumo  # litros por 100 km

    def eficiencia(self):
        return 100 / self.consumo  # km por litro
    def combustible(self, km):
        return (self.consumo * km) / 100  # litros
class Automovil(Vehiculo):
    pass
class Motocicleta(Vehiculo):
    pass

